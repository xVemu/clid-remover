{
    "name": "Clid Remover",
    "version": "1.2",
    "description": "Removes ?fbclid and ?gclid from link when opening page from Facebooks or Googles ad.",
    "manifest_version": 2,
    "author": "Vemu (Kamil Kras)",
    "icons": {
        "16": "img/16x.png",
        "24": "img/24x.png",
        "32": "img/32x.png",
        "48": "img/48x.png",
        "64": "img/64x.png",
        "128": "img/128x.png"
    },
    "permissions": [
        "tabs"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    }
}