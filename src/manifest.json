{
    "name": "fbclid remover",
    "version": "1.0",
    "description": "Removes ?fbclid from link when opening page from Facebook.",
    "manifest_version": 2,
    "author": "Vemu (Kamil Kras)",
    "icons": {
        "128": "./icons/128.png",
        "48": "./icons/48.png",
        "16": "./icons/16.png"
    },
    "permissions": [
        "tabs"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}